	eeprom1: qcom,eeprom@50 {
		cell-index = <1>;
		reg = <0x1>;
		qcom,eeprom-name = "zte_gt24c64a";
		compatible = "qcom,eeprom";
		qcom,i2c-freq-mode = <1>;
		qcom,slave-addr = <0xa0>;
		qcom,cci-master = <0>;
		qcom,num-blocks = <9>;
		qcom,page0 = <0 0x0 2 0x0 1 0>;
		qcom,poll0 = <0 0x0 2 0x0 1 0>;
		qcom,mem0 = <1 0x0cf3 2 0 1 0>;

		qcom,page1 = <0 0x0 2 0x0 1 0>;
		qcom,poll1 = <0 0x0 2 0x0 1 0>;
		qcom,mem1 = <8 0x0001 2 0 1 0>;

		qcom,page2 = <0 0x0 2 0x0 1 0>;
		qcom,poll2 = <0 0x0 2 0x0 1 0>;
		qcom,mem2 = <13 0x0cf4 2 0 1 0>;

		qcom,page3 = <0 0x0 2 0x0 1 0>;
		qcom,poll3 = <0 0x0 2 0x0 1 0>;
		qcom,mem3 = <1770 0x0d01 2 0 1 0>;

		qcom,page4 = <0 0x0 2 0x0 1 0>;
		qcom,poll4 = <0 0x0 2 0x0 1 0>;
		qcom,mem4 = <8 0x0ce6 2 0 1 0>;

		qcom,page5 = <0 0x0 2 0x0 1 0>;
		qcom,poll5 = <0 0x0 2 0x0 1 0>;
		qcom,mem5 = <7 0x13eb 2 0 1 0>;

		qcom,page6 = <0 0x0 2 0x0 1 0>;
		qcom,poll6 = <0 0x0 2 0x0 1 0>;
		qcom,mem6 = <262 0x13f2 2 0 1 0>;

		qcom,page7 = <0 0x0 2 0x0 1 0>;
		qcom,poll7 = <0 0x0 2 0x0 1 0>;
		qcom,mem7 = <263 0x14f8 2 0 1 0>;

		qcom,page8 = <0 0x0 2 0x0 1 0>;
		qcom,poll8 = <0 0x0 2 0x0 1 0>;
		qcom,mem8 = <3 0x17f3 2 0 1 0>;

		cam_vdig-supply = <&pm8916_s4>;
		cam_vana-supply = <&pm8916_l17>;
		cam_vio-supply = <&pm8916_l16>;
		cam_vaf-supply = <&pm8916_l10>;
		qcom,cam-vreg-name = "cam_vdig", "cam_vio", "cam_vana",
										"cam_vaf";
		qcom,cam-vreg-min-voltage = < 0 1800000 2850000 2800000>;
		qcom,cam-vreg-max-voltage = < 0 1800000 2850000 2800000>;
		qcom,cam-vreg-type = <1 0 0 0>;
		qcom,cam-vreg-op-mode = <0	200000 80000 100000>;
		pinctrl-names = "cam_default", "cam_suspend";
		pinctrl-0 = <&cam_sensor_mclk0_default &cam_sensor_rear_default>;
		pinctrl-1 = <&cam_sensor_mclk0_sleep &cam_sensor_rear_sleep>;
		
		qcom,gpio-no-mux = <0>;
		gpios = <&msm_gpio 26 0>,
		<&msm_gpio 34 0>,
			<&msm_gpio 35 0>,
							<&msm_gpio 16 0>,
							<&msm_gpio 93 0>;
					qcom,gpio-reset = <2>;
					qcom,gpio-standby = <1>;
					qcom,gpio-vdig = <3>;
					qcom,gpio-vana = <4>;
					qcom,gpio-req-tbl-num = <0 1 2 3 4>; 	
					qcom,gpio-req-tbl-flags = <1 0 0 0 0>;
					qcom,gpio-set-tbl-delay = <1000 1000 1000 1000 4000>;
					qcom,gpio-req-tbl-label = "CAMIF_MCLK",
							"CAM_RESET1",
							"CAM_STANDBY",
							"CAM_VDIG",
							"CAM_VANA";
		/////////


		qcom,cam-power-seq-type = "sensor_vreg", "sensor_vreg", "sensor_vreg",
			"sensor_vreg", "sensor_gpio", "sensor_gpio", "sensor_gpio", "sensor_gpio", "sensor_clk";
		qcom,cam-power-seq-val = "cam_vdig", "cam_vana", "cam_vio", "cam_vaf",
			"sensor_gpio_reset", "sensor_gpio_standby", "sensor_gpio_vdig", "sensor_gpio_vana", "sensor_cam_mclk";
		qcom,cam-power-seq-cfg-val = <1 1 1 1 1 1 1 1 24000000>;
		qcom,cam-power-seq-delay = <1 1 1 1 1 1 1 30 5>;
		clocks = <&clock_gcc clk_mclk0_clk_src>,
				<&clock_gcc clk_gcc_camss_mclk0_clk>;
		clock-names = "cam_src_clk", "cam_clk";
	};
	
	eeprom0: qcom,eeprom@0 {
		compatible = "zte,eeprom-t4k35";
		cell-index = <0>;
		reg = <0x0>;
		qcom,slave-addr = <0x6c>;
		qcom,cci-master = <0>;
		qcom,eeprom-name = "common_t4k35";	

		cam_vio-supply = <&pm8916_l16>;
		qcom,cam-vreg-name = "cam_vio";
		qcom,cam-vreg-min-voltage = <1800000>;
		qcom,cam-vreg-max-voltage = <1800000>;
		qcom,cam-vreg-op-mode = <0>;
		pinctrl-names = "cam_default", "cam_suspend";
		pinctrl-0 = <&cam_sensor_mclk1_default &cam_sensor_front_eeprom_default>;
		pinctrl-1 = <&cam_sensor_mclk1_sleep &cam_sensor_front_eeprom_sleep>;
		gpios = <&msm_gpio 27 0>,
				<&msm_gpio 28 0>,
				<&msm_gpio 33 0>,
				//<&msm_gpio 93 0>,
				<&msm_gpio 94 0>;
		qcom,gpio-reset = <1>;
		qcom,gpio-standby = <2>;
		//qcom,gpio-vana = <3>;
		qcom,gpio-vdig = <3>;
		qcom,gpio-req-tbl-num = <0 1 2 3>;
		qcom,gpio-req-tbl-flags = <1 0 0 0>;
		qcom,gpio-req-tbl-label = "CAMIF1_MCLK",
				"CAM1_RESET",
				"CAM1_STANDBY",
				//"CAM_VANA",
				"CAM1_VDIG";
				
        qcom,cam-power-seq-type =
                                "sensor_gpio",//reset down
								"sensor_gpio",//standby down
								"sensor_vreg",//iovdd
								//"sensor_gpio",//avdd down
								//"sensor_gpio",//avdd up
								"sensor_gpio",//dvdd down
								"sensor_gpio",//dvdd up
								"sensor_clk",//clk
								"sensor_gpio",//standby up
								"sensor_gpio";//reset up
					  
		qcom,cam-power-seq-val = 
		                        "sensor_gpio_reset",
								"sensor_gpio_standby",
								"cam_vio",
								//"sensor_gpio_vana",
								//"sensor_gpio_vana",
								"sensor_gpio_vdig",
								"sensor_gpio_vdig",
								"sensor_cam_mclk",								
								"sensor_gpio_standby",
								"sensor_gpio_reset";
 
		qcom,cam-power-seq-cfg-val = < 0 0 0  0 1 24000000 1 1>;
		qcom,cam-power-seq-delay = < 2 2 1  1 1 5 1 1>;

		zte,num-blocks = <2>;
		zte,mem0    = <20	 0 2 0x0 1 0>;	//id and golden data
		zte,mem1    = <62	 0 2 0x0 1 0>;  //lsc and awb
		
		status = "ok";			
		clocks = <&clock_gcc clk_mclk1_clk_src>,
				<&clock_gcc clk_gcc_camss_mclk1_clk>;
		clock-names = "cam_src_clk", "cam_clk";	
	};

